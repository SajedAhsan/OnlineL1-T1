#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int tmp = n;
    // extracting the digits
    int digit1 = n%10;
    n=n/10;
    int digit2 = n%10;
    n=n/10;
    int digit3=n%10;
    // taking cube of digits and adding them
    int sum = (digit1*digit1*digit1)+(digit2*digit2*digit2)+(digit3*digit3*digit3);
    if(sum==tmp){
        printf("Armstrong Number");
    }else{
         printf("Not Armstrong Number");

    }
}
